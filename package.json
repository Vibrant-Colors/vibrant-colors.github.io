{
  "name": "color-picker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "node-vibrant": "^3.1.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.10",
    "@storybook/addon-essentials": "^6.4.10",
    "@storybook/addon-links": "^6.4.10",
    "@storybook/builder-webpack5": "^6.4.10",
    "@storybook/manager-webpack5": "^6.4.10",
    "@storybook/node-logger": "^6.4.10",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.10",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "webpack": "^5.65.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
